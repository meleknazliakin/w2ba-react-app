{
	"extends": ["next/core-web-vitals"],
	"env": {
		"browser": true,
		"es2021": true
},
"parserOptions": {
		"ecmaFeatures": {
				"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
},
"plugins": [
		"react"
],
"rules": {
		"react/prop-types": "off",
		"prefer-arrow-callback": "error",
		"object-curly-spacing": [
				"error",
				"always"
		],
		"no-console": 0,
		"no-dupe-else-if": "error",
		"no-duplicate-case": "error",
		"no-empty": "error",
		"no-extra-parens": "error",
		"no-extra-semi": "error",
		"react-hooks/exhaustive-deps": "off",
		"indent": [
				"warn",
				"tab"
		],
		"linebreak-style": [
				"warn",
				"unix"
		],
		"quotes": [
				"warn",
				"single"
		],
		"semi": [
				"error",
				"always"
		]
	}
}